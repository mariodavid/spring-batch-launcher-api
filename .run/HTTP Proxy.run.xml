<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="HTTP Proxy" type="SpringBootApplicationConfigurationType" factoryName="Spring Boot" folderName="Proxy">
    <module name="job-app-http-proxy" />
    <option name="SPRING_BOOT_MAIN_CLASS" value="com.company.batch.sample.app.http.proxy.JobAppHttpProxyApplication" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="de.faktorzehn.batch.sample.app.http.proxy.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>